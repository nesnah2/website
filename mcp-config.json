{
  "mcpServers": {
    "firecrawl": {
      "command": "npx",
      "args": ["@mseep/firecrawl-mcp"],
      "cwd": ".",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dataforseo": {
      "command": "npx",
      "args": ["dataforseo-mcp-server"],
      "cwd": ".",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "github": {
      "command": "node",
      "args": ["github-mcp-server.js"],
      "cwd": ".",
      "env": {
        "NODE_ENV": "production",
        "GITHUB_TOKEN": "your_github_token_here"
      }
    }
  }
} 