{
  "name": "mens-mentoring-website",
  "version": "2.0.0",
  "description": "Professional Men's Mentoring Website with Advanced Features",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss src/css/main.css -o dist/css/main.min.css",
    "build:js": "webpack --mode production",
    "optimize": "npm run optimize:images && npm run optimize:assets",
    "optimize:images": "imagemin assets/* --out-dir=dist/assets",
    "optimize:assets": "copyfiles -u 1 assets/* dist/assets/",
    "deploy": "npm run build && npm run optimize"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.29.2",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@mseep/firecrawl-mcp": "^1.9.0",
    "dataforseo-mcp-server": "^2.7.2",
    "octokit": "^3.1.2",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "express": "^4.18.2",
    "compression": "^1.7.4",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.1.5",
    "nodemailer": "^6.9.7",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "redis": "^4.6.10",
    "stripe": "^14.7.0",
    "twilio": "^4.19.0",
    "aws-sdk": "^2.1490.0",
    "sharp": "^0.32.6",
    "web-push": "^3.6.6",
    "socket.io": "^4.7.4",
    "bull": "^4.12.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "terser-webpack-plugin": "^5.3.9",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack-manifest-plugin": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "core-js": "^3.33.2",
    "imagemin": "^8.0.1",
    "imagemin-cli": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "mens-mentoring",
    "coaching",
    "personal-development",
    "professional-website",
    "nodejs",
    "express"
  ],
  "author": "Mikkel",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
